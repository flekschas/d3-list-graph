{
  "env": {
    "browser": true,
    "es6": true
  },
  "global": {
    "angular": false,
    "d3": false
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [2, 2, {"VariableDeclarator": { "var": 2, "let": 2, "const": 3}, "SwitchCase": 1}],
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "always"],
    "space-before-function-paren": [2, "always"],
    "space-after-keywords": [2, "always"],
    "strict": [2, "global"]
  }
}
